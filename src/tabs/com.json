{
  "title": "Tabs",
  "namespace": "mybricks.taro.tabs",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "description": "tabs",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "icon": "./icon.svg",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "dataSource",
      "title": "设置数据源",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "title": "标签项ID",
              "type": "string"
            },
            "tabName": {
              "title": "标签项名称",
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "activeTabId",
      "title": "激活tab项",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "getActiveTabId",
      "title": "获取激活tab项",
      "schema": {
        "type": "any"
      },
      "rels": ["activeTabId"]
    },
    {
      "id": "setBadge",
      "title": "设置徽标",
      "schema": {
        "type": "object",
        "properties": {
          "index": {
            "type": "number"
          },
          "text": {
            "type": "string"
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "changeTab",
      "title": "切换tab",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "activeTabId",
      "title": "tab激活项",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "tabName": {
            "type": "string"
          },
          "index": {
            "type": "number"
          }
        }
      }
    }
  ],
  "slots": [
    {
      "id": "tabName1",
      "capacity": 1,
      "title": "内容项1",
      "layout": "smart",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项数据",
          "schema": {
            "type": "object"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    },
    {
      "id": "tabName2",
      "capacity": 1,
      "title": "内容项2",
      "layout": "smart",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项数据",
          "schema": {
            "type": "object"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    }
  ]
}
