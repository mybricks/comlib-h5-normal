{
  "title": "排序",
  "namespace": "mybricks.taro.filterSort",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "icon": "./icon.svg",
  "preview": "./preview.png",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置排序的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置排序的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取排序的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": ["returnValue"]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空排序的值",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    }
  ],
  "schema": ["mybricks.taro.filters/item", "*"]
}
