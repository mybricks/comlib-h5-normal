{
  "title": "服务接口",
  "namespace": "mybricks.taro._connector",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "description": "连接器",
  "runtime": "./runtime.ts",
  "editors": "./editors.tsx",
  "data": "./data.json",
  "icon": "./icon.svg",
  "rtType": "js-autorun",
  "inputs": [
    {
      "id": "call",
      "rels": ["then", "catch"],
      "title": "调用",
      "schema": {
        "type": "object"
      }
    },
    {
      "id": "setGlobalHeaders",
      "title": "全局设置额外请求头",
      "schema": {
        "type": "object"
      }
    },
    {
      "id": "setGlobalParams",
      "title": "全局设置额外请求参数",
      "schema": {
        "type": "object"
      }
    },
    {
      "id": "setGlobalBody",
      "title": "全局设置额外请求体",
      "schema": {
        "type": "object"
      }
    }
  ],
  "outputs": [
    {
      "id": "then",
      "title": "结果",
      "schema": {
        "type": "unknown"
      }
    },
    {
      "id": "catch",
      "title": "发生错误",
      "schema": {
        "type": "string"
      }
    }
  ]
}
