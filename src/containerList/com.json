{
  "title": "循环列表",
  "namespace": "mybricks.taro.containerList",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "runtime.edit": "./edit/runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "icon": "./icon.svg",
  "inputs": [
    {
      "id": "refreshDataSource",
      "title": "覆盖数据",
      "desc": "列表已有的数据也会被覆盖",
      "schema": {
        "title": "列表数据",
        "type": "array",
        "items": {
          "title": "列项数据",
          "type": "any"
        }
      }
    },
    {
      "id": "addDataSource",
      "title": "添加数据",
      "desc": "在列表数据的末尾插入添加的数据",
      "schema": {
        "title": "列表数据",
        "type": "array",
        "items": {
          "title": "列项数据",
          "type": "any"
        }
      }
    },
    {
      "id": "loading",
      "title": "切换到加载中",
      "desc": "提示用户正在加载中",
      "schema": {
        "title": "列表数据",
        "type": "boolean"
      }
    },
    {
      "id": "noMore",
      "title": "切换到加载完毕",
      "desc": "提示用户已经没有数据了",
      "schema": {
        "title": "列表数据",
        "type": "boolean"
      }
    },
    {
      "id": "error",
      "title": "切换到加载报错",
      "desc": "提示用户报错信息",
      "schema": {
        "title": "列表数据",
        "type": "boolean"
      }
    },
    {
      "id":"empty",
      "title":"切换到无内容",
      "desc":"提示用户列表没有内容",
       "schema":{
        "title":"列表数据",
        "type":"boolean"
      }
    }
  ],
  "outputs": [
    {
      "id": "onScrollLoad",
      "title": "当滚动加载时"
    },
    {
      "id": "onRefresh",
      "title": "当下拉刷新时"
    }
  ],
  "slots": [
    {
      "id":"emptySlot",
      "title":"无内容插槽"
    },
    {
      "id": "item",
      "capacity": 1,
      "title": "列表项",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项",
          "desc": "当前项展示内容",
          "schema": {
            "type": "any"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "desc": "当前项展示的序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    }
  ]
}