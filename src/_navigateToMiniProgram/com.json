{
  "title": "打开其他小程序",
  "namespace": "mybricks.taro.navigateToMiniProgram",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "description": "打开其他小程序",
  "icon": "./icon.svg",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "runtime": "./runtime.tsx",
  "rtType": "js",
  "inputs": [
    {
      "id": "open",
      "title": "打开",
      "schema": {
        "type": "object",
        "properties": {
          "appId": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "extraData": {
            "type": "object"
          },
          "envVersion": {
            "type": "string"
          },
          "shortLink": {
            "type": "string"
          },
          "noRelaunchIfPathUnchanged": {
            "type": "boolean"
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "onSuccess",
      "title": "成功",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "onFail",
      "title": "失败",
      "schema": {
        "type": "any"
      }
    }
  ]
