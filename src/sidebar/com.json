{
  "title": "侧边栏",
  "namespace": "mybricks.taro.sidebar",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "description": "侧边栏",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "icon": "./icon.svg",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "setSidebarData",
      "title": "配置侧边选项",
      "schema": {
        "title": "列表数据",
        "type": "array",
        "items": {
          "title": "列项数据",
          "type": "object",
          "properties": {
            "tabName": {
              "type": "string"
            }
          }
        }
      },
      "rels": [
        "afterSetSidebarData"
      ]
    },
    {
      "id": "activeTabName",
      "desc":"输入tab名称，激活对应的tab项",
      "title": "激活tab项",
      "schema": {
        "type": "string"
      }
    }
  ],
  "outputs": [
    {
      "id": "changeTab",
      "title": "切换tab",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "afterSetSidebarData",
      "title": "数据设置后",
      "schema": {
        "title": "列表数据",
        "type": "array",
        "items": {
          "title": "列项数据",
          "type": "any"
        }
      }
    }
  ],
  "slots": [
    {
      "id":"topSlot",
      "title":"顶部插槽"
    },
    {
      "id": "content",
      "title": "内容"
    },
    {
      "id": "tabItem",
      "capacity": 1,
      "title": "动态标签项",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项",
          "desc": "当前项展示内容",
          "schema": {
            "type": "any"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "desc": "当前项展示的序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    },
    {
      "id": "tabName1",
      "title": "标签项1",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "传入的数据",
          "schema": {
            "type": "any"
          }
        }
      ]
    },
    {
      "id": "tabName2",
      "title": "标签项2",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "传入的数据",
          "schema": {
            "type": "any"
          }
        }
      ]
    }
  ]
}