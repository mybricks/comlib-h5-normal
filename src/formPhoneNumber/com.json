{
  "title": "手机输入框",
  "namespace": "mybricks.taro.formPhoneNumber",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "icon": "./icon.svg",
  "preview": "./preview.png",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置单行输入的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置单行输入的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发单行输入的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取单行输入的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空单行输入的值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "getRealtimePhoneNumberSuccess",
      "title": "获取动态令牌成功",
      "schema": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "errMsg": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "getRealtimePhoneNumberFail",
      "title": "获取动态令牌失败",
      "schema": {
        "type": "object",
        "properties": {
          "errno": {
            "type": "number"
          },
          "errMsg": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "当输入的手机号变化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onCodeSend",
      "title": "获取验证码",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onPressEnter",
      "title": "按下回车",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    }
  ],
  "schema": [
    "mybricks.taro.formContainer/formItem",
    "*"
  ]
}