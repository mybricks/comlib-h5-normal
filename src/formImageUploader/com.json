{
  "title": "图片上传",
  "namespace": "mybricks.taro.formImageUploader",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "icon": "https://ali-ec.static.yximgs.com/udata/pkg/eshop/fangzhou/temp/adafadsf.9c4267d32a661af5.svg",
  "preview": "https://f2.eckwai.com/kos/nlav12333/fangzhou/pub/compress/image-7dfd081d-1711-416c-9d72-b4711c17ecf3.png",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置值，并触发值变化事件输出",
      "schema": {
        "type": "array"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置初始值，并触发初始化事件输出",
      "schema": {
        "type": "array"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "array"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "array"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "array"
      }
    }
  ],
  "schema": [
    "mybricks.taro.formContainer/formItem",
    "*"
  ]
}