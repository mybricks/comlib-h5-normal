{
  "title": "轮播容器",
  "namespace": "mybricks.taro.containerSwiper",
  "author": "mybricks-team",
  "author_name": "板砖团队",
  "version": "1.0.0",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "icon": "./icon.svg",
  "inputs": [
    {
      "id": "dataSource",
      "title": "设置数据源",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "title": "轮播项ID",
              "type": "string"
            },
            "title": {
              "title": "名称",
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "slots": [
    {
      "id": "cont_1",
      "capacity": 1,
      "title": "轮播项1",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项数据",
          "schema": {
            "type": "object"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    },
    {
      "id": "cont_2",
      "capacity": 1,
      "title": "轮播项2",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项数据",
          "schema": {
            "type": "object"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    }
  ]
}
